          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT team,\r\n       SUM(CASE WHEN result = 'win' THEN 1 ELSE 0 END) AS wins,\r\n       SUM(CASE WHEN result = 'loss' THEN 1 ELSE 0 END) AS losses,\r\n       SUM(CASE WHEN result = 'draw' THEN 1 ELSE 0 END) AS draws\r\nFROM (\r\n    SELECT winner AS team, 'win' AS result\r\n    FROM matches\r\n    UNION ALL\r\n    SELECT team1 AS team, CASE WHEN winner = team2 THEN 'loss' ELSE 'draw' END AS result\r\n    FROM matches\r\n    WHERE winner IS NULL OR winner != team1\r\n    UNION ALL\r\n    SELECT team2 AS team, CASE WHEN winner = team1 THEN 'loss' ELSE 'draw' END AS result\r\n    FROM matches\r\n    WHERE winner IS NULL OR winner != team2\r\n) AS results\r\nGROUP BY team\r\nORDER BY wins DESC, losses ASC;\r\n",
          "refId": "A",
          "select": [
            [
              {
                "params": [
                  "value"
                ],
                "type": "column"
              }
            ]
